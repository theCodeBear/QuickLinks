{
  "manifest_version": 2,

  "name": "QuickLinks",
  "description": "Speed dial for the internet.",
  "version": "1.0",

  "omnibox": {"keyword":"ql"},

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}

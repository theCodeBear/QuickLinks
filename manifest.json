{
  "manifest_version": 2,

  "name": "QuickLinks",
  "description": "Speed dial for the internet.",
  "version": "1.0",

  "omnibox": {"keyword":"ql"},

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}
